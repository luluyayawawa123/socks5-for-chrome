{
  "manifest_version": 3,
  "name": "Socks5 代理切换器",
  "version": "1.0",
  "description": "快速切换本地 Socks5 代理",
  "permissions": [
    "proxy",
    "storage",
    "host_permissions"
  ],
  "host_permissions": [
    "https://www.google.com/"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 